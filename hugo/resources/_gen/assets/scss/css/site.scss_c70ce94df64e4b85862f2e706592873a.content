  font-weight: 900 !important; }

.text--inherit {
  font-weight: inherit !important; }

/* TEXT STYLES */
.text--hairline {
  font-style: 100 !important; }

.text--extralight {
  font-style: 200 !important; }

.text--light {
  font-style: 300 !important; }

.text--normal {
  font-style: 400 !important; }

.text--medium {
  font-style: 500 !important; }

.text--semibold {
  font-style: 600 !important; }

.text--bold {
