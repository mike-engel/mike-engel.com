:root {
  --foreground: #0B2940;
  --background: #FFFFFF;
  --lighter: #7B92A4;
  --darker: #355873; }

.footer {
  padding: 2.666em 0 1.333em 0; }
  .footer a {
    margin-left: 1em; }

.header nav {
  display: flex;
  flex-direction: column;
  padding: 20px 0; }

.header ul {
  margin: 0.5em 0 0 0; }

.header li {
  display: inline-block;
  margin-right: 1em;
  text-transform: lowercase; }

.header svg {
  width: auto;
  height: 50px;
  margin-right: 1.5em;
  fill: var(--foreground); }

@media (min-width: 768px) {
  .header nav {
    flex-direction: row;
    align-items: center; }
  .header li {
    margin-right: 1.5em; }
  .header ul {
    margin: -10px 0 0 0; } }

.recent-projects h3,
.recent-projects p {
  margin: 0;
  padding: 0; }

.recent-projects p {
  margin-top: 0.5em; }

.recent-projects p + h3 {
  margin-top: 1em; }

.recent-projects li {
  display: inline-block;
  width: 100%;
  margin-bottom: 1.5em;
  vertical-align: top; }

@media (min-width: 768px) {
  .recent-projects li {
    margin-right: 2em;
    width: calc(50% - 2em); }
  .recent-projects li:nth-child(2n) {
    margin-right: 0; } }

@media (min-width: 1024px) {
  .recent-projects li {
    margin-right: 2em;
    width: calc(33.3333333333% - 2em); }
  .recent-projects li:nth-child(2n) {
    margin-right: 2em; }
  .recent-projects li:nth-child(3n) {
    margin-right: 0; } }

.contact-form {
  max-width: 40em; }
  .contact-form > span, .contact-form > div {
    position: relative; }
  .contact-form > span + span, .contact-form > div + div {
    margin-top: 1em; }
  .contact-form fieldset {
    margin: 0;
    padding: 0;
    border: none; }
    .contact-form fieldset > span, .contact-form fieldset > div {
      position: relative; }
    .contact-form fieldset > span + span, .contact-form fieldset > div + div {
      margin-top: 1em; }
  .contact-form button {
    background: transparent;
    border: 2px solid var(--foreground);
    border-radius: 1px;
    font: inherit;
    font-size: 1rem;
    font-weight: 700;
    letter-spacing: 0.03em;
    line-height: 1;
    margin-top: 1em;
    padding: 10px 15px; }
  .contact-form label {
    box-sizing: border-box;
    display: block;
    width: 100%;
    font-family: -apple-system, BlinkMacSystemFont, Segoe UI, Roboto, Oxygen, Ubuntu, Cantarell, Fira Sans, Droid Sans, Helvetica Neue, sans-serif;
    padding-top: 5px;
    position: relative; }
  .contact-form label + label {
    margin-top: 0.5em; }
  .contact-form input,
  .contact-form textarea {
    border: none;
    border-bottom: 2px solid var(--foreground);
    box-sizing: border-box;
    font-family: -apple-system, BlinkMacSystemFont, Segoe UI, Roboto, Oxygen, Ubuntu, Cantarell, Fira Sans, Droid Sans, Helvetica Neue, sans-serif;
    font-size: 1rem;
    padding: 12px 0 8px 0;
    width: 100%; }
  .contact-form textarea {
    min-height: 150px; }
  .contact-form label span {
    box-sizing: border-box;
    font-size: 0.625rem;
    left: 0;
    padding: 0;
    pointer-events: none;
    position: absolute;
    top: 0;
    transition: font-size 200ms, padding 200ms;
    z-index: 1; }

.single-photo {
  margin-top: 2em; }
  .single-photo h1 {
    margin-top: 0;
    margin-bottom: 0.3em;
    padding: 0; }
  .single-photo p {
    margin-bottom: 3em; }
  .single-photo figure {
    position: relative;
    margin: 0; }
    .single-photo figure img {
      display: block;
      width: 100%;
      height: calc(attr(data-height-at-1200) * 100% / 1200);
      margin-bottom: 2em; }
    .single-photo figure .meta-container {
      display: flex;
      flex-direction: column;
      position: relative; }
    .single-photo figure figcaption {
      max-width: 40em;
      padding: 0 0 2em 0; }
    .single-photo figure ul {
      margin: 0;
      overflow: hidden;
      width: 100%; }
    .single-photo figure ul li {
      display: flex;
      justify-content: space-between;
      align-items: center;
      flex-direction: row;
      position: relative; }
    .single-photo figure ul li + li {
      margin-top: 0.75em; }
    .single-photo figure ul li:after {
      position: absolute;
      bottom: 0;
      left: 0;
      width: 0;
      white-space: nowrap;
      font-weight: 300;
      line-height: 1;
      color: var(--lighter);
      content: ". . . . . . . . . . . . . . . . . . . . " ". . . . . . . . . . . . . . . . . . . . " ". . . . . . . . . . . . . . . . . . . . " ". . . . . . . . . . . . . . . . . . . . "; }
    .single-photo figure span:first-child,
    .single-photo figure span:last-child {
      font-size: 0.8125rem;
      background: var(--background);
      display: block;
      margin: 0;
      padding: 0;
      line-height: 1;
      position: relative;
      z-index: 100; }
    .single-photo figure span:first-child {
      padding: 0 0.3em 0 0;
      font-weight: 400; }
    .single-photo figure span:last-child {
      text-align: right;
      padding: 0 0 0 0.3em;
      font-weight: 600; }
    @media (min-width: 768px) {
      .single-photo figure figure + figure {
        margin-top: 5em; }
      .single-photo figure .meta-container {
        flex-direction: row; }
      .single-photo figure figcaption,
      .single-photo figure ul {
        flex: 1 100%; }
      .single-photo figure figcaption {
        flex: 1 66%;
        padding: 0 1em 0 0; }
      .single-photo figure ul {
        flex: 1 33%; } }
.gallery {
  margin-top: 2em; }
  .gallery__year {
    max-width: unset; }
    .gallery__year h2 {
      border-top: 2px solid var(--foreground);
      padding: 0.25em 0;
      margin: 0 0 0.5em 0;
      width: 100%;
      max-width: unset; }
  .gallery__year + .gallery__year {
    margin-top: 4em; }
  .gallery__photos {
    display: grid;
    grid-template-columns: 1fr 1fr;
    grid-row-gap: 0.5em;
    grid-column-gap: 0.5em; }
    @media (min-width: 768px) {
      .gallery__photos {
        grid-template-columns: 1fr 1fr 1fr; } }
    @media (min-width: 1024px) {
      .gallery__photos {
        grid-template-columns: 1fr 1fr 1fr 1fr; } }
    @media (min-width: 1200px) {
      .gallery__photos {
        grid-template-columns: 1fr 1fr 1fr 1fr 1fr; } }
  .gallery__photo {
    line-height: 0; }
    .gallery__photo img {
      display: block;
      width: 100%; }

.resume ul:not(.plain-list) {
  list-style: disc; }

.resume h3 {
  margin-bottom: 0.25em; }

.resume .logo,
.resume .resume-meta {
  display: inline-block;
  vertical-align: top; }

.resume .logo {
  display: none;
  width: auto;
  height: 50px;
  margin-right: 1.7em;
  fill: var(--foreground); }

.resume .resume-header {
  padding-top: 1px; }

.resume .resume-meta {
  margin: 0; }
  .resume .resume-meta li {
    font-size: 0.777778rem;
    line-height: 1.4; }

.resume .resume-summary {
  margin: 2em 0 0.5em 0;
  font-weight: 400;
  max-width: 40em;
  line-height: 1.4; }

.resume .resume-summary ~ h2 {
  margin: 1em 0 0.5em 0; }

.resume .resume-experience > li + li,
.resume .resume-education > li + li {
  margin-top: 2em; }

.resume .resume-experience h3,
.resume .resume-education h3 {
  margin: 0 0 0.2em 0; }

.resume .resume-experience p, .resume .resume-education p {
  margin: 0; }

.resume .links {
  margin-top: 1em; }

.resume h3 + p {
  margin-top: 0; }

.resume .hide-from-screen {
  display: none; }
  @media (print) {
    .resume .hide-from-screen {
      display: initial; } }
@media print {
  body > header {
    display: none; }
  html,
  body {
    font-size: 10pt; }
  body {
    line-height: 1.3; }
  .site-constraint {
    width: 670px; }
  main + footer {
    display: none !important; }
  .back-button {
    display: none !important; }
  h2,
  h3 {
    padding: 0 !important; }
  h2 {
    font-size: 1.25rem; }
  h3 {
    font-size: 1rem;
    margin-top: .25em !important; }
  p {
    line-height: 1.4; }
  p + p {
    margin: 1em 0; }
  a {
    border: none; }
  h2 > * {
    margin-bottom: 1em; }
  h2 + h3 {
    margin-top: 0; }
  h3 + p {
    margin-bottom: .75em; }
  li, p, .resume-summary {
    max-width: unset !important;
    line-height: 1.3 !important; }
  .logo {
    display: initial !important;
    height: 35px !important;
    margin-right: 1em !important; }
  .resume-meta li:first-child {
    font-weight: bold;
    font-size: 1rem; }
  .resume-summary {
    margin-top: 1.5em !important;
    margin-bottom: 0 !important; }
  .resume-summary ~ h2 {
    margin-top: 1.75em !important;
    margin-bottom: 0.5em !important; }
  .resume-experience li + li,
  .resume-education li + li {
    margin-top: 1.25em !important; }
  ul:not(.plain-list) {
    margin: 0.5em 0 0 1em !important; }
    ul:not(.plain-list) li + li {
      margin-top: .25em !important; }
  .hide-from-print {
    display: none; }
  .hide-from-screen {
    display: initial; } }

body {
  color: var(--foreground);
  background-color: var(--background);
  font-family: system-ui, -apple-system, BlinkMacSystemFont, "Segoe UI", "Roboto", "Oxygen", "Ubuntu", "Cantarell", "Fira Sans", "Droid Sans", "Helvetica Neue", sans-serif;
  font-size: 16px;
  font-weight: 400;
  line-height: 1.6; }

.site-constraint {
  width: 90vw;
  margin: 0 auto; }

ul:not(.plain-list) {
  margin: 1em 0 0 1em;
  padding-left: initial; }
  ul:not(.plain-list) li + li {
    margin-top: 0.5em; }

li {
  max-width: 40em; }

dt,
dd {
  margin-left: 0;
  padding-left: 0; }

dd + dt {
  margin-top: 1em; }

dt {
  font-weight: 700; }

pre {
  padding: 3px;
  border-radius: 3px;
  background-color: #f8f8f8;
  padding: .5em 1em;
  overflow-x: scroll; }

@media (min-width: 528px) {
  .site-constraint {
    width: calc(528px - 7vw); } }

@media (min-width: 768px) {
  .site-constraint {
    width: calc(768px - 7vw); } }

@media (min-width: 1024px) {
  body {
    font-size: 18px; }
  html {
    font-size: 1.125rem; }
  .site-constraint {
    width: calc(1024px - 7vw); } }

@media (min-width: 1200px) {
  .site-constraint {
    width: calc(1200px - 7vw); } }
